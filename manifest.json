
{
	"name": "Download table as CSV",
    "author":"Rohit Yadav",
	"description": "Download HTML tables in CSV file.",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": ["contextMenus", "activeTab"],
    "background": {
		"scripts": ["content.js"],
		"persistent": false
	},
    "browser_action": {
		"default_popup": "csv.html"
	},
	"icons": {
		"32": "image/32.png",
		"128": "image/128.png"
	}
}